{
  "name": "ts-prisma-docker-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts --config eslint.config.mjs",
    "format": "prettier --write . --config prettier.config.mjs",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:create": "npx prisma migrate dev --name",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "setup": "pnpm install && pnpm db:deploy && pnpm run dev",
    "docker:up": "docker-compose up --build -d",
    "docker:down": "docker-compose down",
    "validate": "pnpm lint && pnpm format --check"
  },
  "dependencies": {
    "@prisma/client": "6.7.0",
    "dotenv": "16.5.0",
    "effect": "^3.14.22",
    "express": "5.1.0",
    "outdated": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/express": "5.0.1",
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "eslint": "9.26.0",
    "prettier": "3.5.3",
    "prisma": "6.7.0",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@8.15.4"
}