{
  "name": "taq-u-app",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter @taq-u-app/api dev & pnpm --filter @taq-u-app/web dev",
    "dev:api": "pnpm --filter @taq-u-app/api dev",
    "dev:web": "pnpm --filter @taq-u-app/web dev",
    "build": "pnpm --filter @taq-u-app/api build && pnpm --filter @taq-u-app/web build",
    "build:api": "pnpm --filter @taq-u-app/api build",
    "build:web": "pnpm --filter @taq-u-app/web build",
    "lint": "pnpm -r lint",
    "format": "prettier --write . --config prettier.config.mjs",
    "db:deploy": "pnpm --filter @taq-u-app/api db:deploy",
    "db:migrate": "pnpm --filter @taq-u-app/api db:migrate",
    "db:migrate:create": "pnpm --filter @taq-u-app/api db:migrate:create",
    "db:reset": "pnpm --filter @taq-u-app/api db:reset",
    "db:studio": "pnpm --filter @taq-u-app/api db:studio",
    "docker:up": "docker-compose up --build -d",
    "docker:down": "docker-compose down",
    "validate": "pnpm lint && pnpm format --check"
  },
  "dependencies": {
    "@prisma/client": "6.7.0",
    "dotenv": "16.5.0",
    "effect": "^3.14.22",
    "express": "5.1.0",
    "outdated": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/express": "5.0.1",
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "eslint": "9.26.0",
    "prettier": "3.5.3",
    "prisma": "6.7.0",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@8.15.4"
}