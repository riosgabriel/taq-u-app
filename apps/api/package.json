{
  "name": "@taq-u-app/api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:create": "npx prisma migrate dev --name",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "6.7.0",
    "dotenv": "16.5.0",
    "effect": "^3.14.22",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@types/express": "5.0.1",
    "prisma": "6.7.0",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3"
  }
}
